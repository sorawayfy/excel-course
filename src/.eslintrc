{
    "parser": "babel-eslint",
    "env": {
        "es6": true,
        "node": true,
        "browser": false
    },
    "rules": {
        "semi": "off",
        "arrow-parens": "off",
        "comma-dangle": "off",
        "require-jsdoc": "off",
        "linebreak-style": "off"
    },
    "extends": [
        "eslint:recommended", 
        "google"
    ]
}